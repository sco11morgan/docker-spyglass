<div class="page-header">
  <h3> <small><%= tag1 %> -> <%= tag2 %>, <%= image1_human_size %> Image </small></h3>
  <h5> <%= percent_reuse %>% efficient (layers reused between tags)</h5>
</div>

<div class="container">
  <div class="row">
    <div class="col"></div>
    <div class="col-9">
      <p class="text-center">Common</p>
      <table class="table table-hover table-sm">
        <tbody>
            <!-- <tr><td colspan="5">Common</td></tr> -->
          <% shared.each do |layer| %>
            <tr class="layer-<%= layer['prefix'] %>">
              <td data-content="<%= layer['created_by'][0..100] %>"  data-trigger="hover" data-toggle="popover" >
                <%= layer['created_by'][0..40] %>
              </td>
              <td class="d-none"><%= layer['created_by'] %></td>
              <td><small><%= layer['created'][0..20] %></small></td>
              <td align="right"><%= layer['human_size'] %></td>
              <td><span class="bullet"><%= layer['size'] %>,<%= layer['max'] %></span></td>
            </tr>
          <% end %>
        </tbody>
      </table>    
    </div>
    <div class="col"></div>
  </div>
  </div>
  <div class="container-fluid">
  <div class="row">
    <div class="col-6">
      <p class="text-center"><%= tag1 %></p>
      <table class="table table-hover table-sm">
        <tbody>
          <% diff1.each do |layer| %>
            <tr class="layer-<%= layer['prefix'] %>">
              <!-- <td><%= layer['prefix'] %></td> -->
              <td><%= layer['created_by'][0..40] %></td>
              <td><small><%= layer['created'][0..20] %></small></td>
              <td align="right"><%= layer['human_size'] %></td>
              <td><span class="bullet"><%= layer['size'] %>,<%= layer['max'] %></span></td>
            </tr>
          <% end %>
        </tbody>
      </table>    
    </div>
    <div class="col-6">
      <p class="text-center"><%= tag2 %></p>
      <table class="table table-hover table-sm">
        <tbody>
          <% diff2.each do |layer| %>
            <tr class="layer-<%= layer['prefix'] %>">
              <!-- <td><%= layer['prefix'] %></td> -->
              <td><%= layer['created_by'][0..40] %></td>
              <td><small><%= layer['created'][0..20] %></small></td>
              <td align="right"><%= layer['human_size'] %></td>
              <td><span class="bullet"><%= layer['size'] %>,<%= layer['max'] %></span></td>
            </tr>
          <% end %>
        </tbody>
      </table>    
    </div>
  </div>
</div>


